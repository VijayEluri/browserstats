   </agent>
   <agent name="python">
     <regex>^Python-urllib/</regex>
     <sample>Python-urllib/1.17</sample>
   </agent>
   <agent name="swish">
     <regex>^swish-e</regex>
     <sample>swish-e http://swish-e.org/</sample>
   </agent>
   <!-- Used for unidentified Nutch crawlers only -->
   <agent name="nutch">
     <regex>^agentname/Nutch</regex>
     <sample>agentname/Nutch-1.0-dev</sample>
   </agent>
  </group>
  
</agentlist>