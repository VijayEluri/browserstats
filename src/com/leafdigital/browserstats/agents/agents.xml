<agentlist>  
  <!-- 
    Mobile phones.
    -->
  <group type="mobile">
    <group name="iphone" engine="webkit" os="iphone">
      <regex>iPhone OS.* AppleWebKit/</regex>
      <agent version="3.0">
        <regex>Version/3\.</regex>
	      <sample>Mozilla/5.0 (iPod; U; CPU iPhone OS 2_2_1 like Mac OS X; en-us) AppleWebKit/525.18.1 (KHTML, like Gecko) Version/3.1.1 Mobile/5H11a Safari/525.20</sample>
      </agent>
      <agent version="4.0">
        <regex>Version/4\.</regex>
        <sample>iPhone; U; CPU iPhone OS 3_0 like Mac OS X; en-us) AppleWebKit/528.18 (KHTML, like Gecko) Version/4.0 Mobile/7A341 Safari/528.16</sample>
      </agent>
    </group>
    
    <!-- The imode user-agent includes a wide range of equipment and versions, 
         but I don't know enough to separate it more usefully. -->
    <agent name="imode" engine="" os="" version="">    
      <regex>^DoCoMo/</regex>
      <sample>DoCoMo/2.0 P90liS(c100;TB;W20H10)</sample>
    </agent>
  </group>
  
  <!-- 
    Video game systems (handheld or consoles).
    -->
  <group type="gamesystem" engine="" os="" version="">
    <agent name="psp">
      <regex>^Mozilla/4\.0 \(PSP \(PlayStation Portable\)</regex>
      <sample>Mozilla/4.0 (PSP (PlayStation Portable); 2.00)</sample>
    </agent>
    <agent name="ps3">
      <regex>^Mozilla/5.0 \(PLAYSTATION 3</regex>
      <sample>Mozilla/5.0 (PLAYSTATION 3; 1.00)</sample>
    </agent>
    <agent name="wii">
      <regex>^Opera/.*\(Nintendo Wii;</regex>
      <sample>Opera/9.30 (Nintendo Wii; U; ; 2047-7; en)</sample>
    </agent>
    <agent name="dsi">
      <regex>^Opera/.*\(Nintendo DSi;</regex>
      <sample>Opera/9.50 (Nintendo DSi; Opera/483; U; en-US)</sample>
    </agent>
  </group>
  
  <!-- 
    Browsers running on standard computers and operating systems.
    -->
  <group type="browser">
	  <group engine="webkit">
	    <regex>AppleWebKit/</regex>
	    
	    <group name="chrome">
	      <regex>Chrome/</regex>
	      <group version="1">
	        <regex>Chrome/1</regex>
		      <agent os="win">
		        <regex>Windows</regex>
		        <sample>Mozilla/5.0 (Windows; U; Windows NT 5.1; en-US) AppleWebKit/525.19 (KHTML, like Gecko) Chrome/1.0.154.48 Safari/525.19</sample>
		      </agent>
	      </group>
	      <group version="2">
	        <regex>Chrome/2</regex>
		      <agent os="win">
		        <regex>Windows</regex>
		        <sample>Mozilla/5.0 (Windows; U; Windows NT 5.1; en-US) AppleWebKit/530.5 (KHTML, like Gecko) Chrome/2.0.172.33 Safari/530.5</sample>
		      </agent>
	      </group>
	    </group>
	    
	    <group name="safari">
	      <regex>Safari/</regex>
	      <group version="3">
	        <regex>Version/3\.</regex>
		      <agent os="mac">
		        <regex>\(Macintosh;</regex>
		        <sample>Mozilla/5.0 (Macintosh; U; Intel Mac OS X 10_5_6; en-us) AppleWebKit/525.27.1 (KHTML, like Gecko) Version/3.2.1 Safari/525.27.1</sample>
		      </agent>
		    </group>
	      <group version="4">
	        <regex>Version/4\.</regex>
		      <agent os="mac">
		        <regex>\(Macintosh;</regex>
		        <sample>Mozilla/5.0 (Macintosh; U; Intel Mac OS X 10_5_7; en-us) AppleWebKit/530.17 (KHTML, like Gecko) Version/4.0 Safari/530.17</sample>
		      </agent>
		      <agent os="win">
		        <regex>\(Windows;</regex>
		        <sample>Mozilla/5.0 (Windows; U; Windows NT 6.0; en-US) AppleWebKit/530.17 (KHTML, like Gecko) Version/4.0 Safari/530.17</sample>
		      </agent>
		    </group>
	    </group>
	  </group>

    <group engine="presto" name="opera">
      <regex>^Opera/</regex>
      <group version="9">
        <regex>^Opera/9\.</regex>
        <agent os="win">
          <regex>Windows</regex>
          <sample>Opera/9.61 (Windows NT 5.1; U; en) Presto/2.1.1</sample>
        </agent>
        <agent os="mac">
          <regex>Macintosh</regex>
          <sample>Opera/9.63 (Macintosh; Intel Mac OS X; U; en) Presto/2.1.1</sample>
        </agent>
      </group>
    </group>
    
    <group engine="trident" name="ie" os="win">
      <regex>^Mozilla/4\.0 \(compatible; MSIE [0-9]+\.[0-9]+;.*Windows</regex> 
      <agent version="5.0">
        <regex>MSIE 5.0</regex>
        <sample>Mozilla/4.0 (compatible; MSIE 5.0; Windows 98)</sample>
      </agent>
      <agent version="5.5">
        <regex>MSIE 5.5</regex>
        <sample>Mozilla/4.0 (compatible; MSIE 5.5; Windows NT 4.0)</sample>
      </agent>
      <agent version="6.0">
        <regex>MSIE 6.0</regex>
        <sample>Mozilla/4.0 (compatible; MSIE 6.0; AOL 9.0; Windows NT 5.1; SV1; .NET CLR 1.0.3705; .NET CLR 1.1.4322; Media Center PC 4.0)</sample>
      </agent>
      <agent version="7.0">
        <regex>MSIE 7.0</regex>
        <sample>Mozilla/4.0 (compatible; MSIE 7.0; Windows NT 5.1; .NET CLR 2.0.50727)</sample>
      </agent>
      <agent version="8.0">
        <regex>MSIE 8.0</regex>
        <sample>Mozilla/4.0 (compatible; MSIE 8.0; Windows NT 6.0; Trident/4.0; SLCC1; .NET CLR 2.0.50727; Media Center PC 5.0; .NET CLR 3.5.30729; .NET CLR 3.0.30618)</sample>
      </agent>
    </group>
    
    <group engine="gecko" name="firefox">
      <regex>^Mozilla/5\.0.* Gecko/.* (Firefox|Iceweasel|Minefield)/.*</regex>
      <group version="1.5">
        <regex>(Firefox||Iceweasel)/1\.5</regex>
        <agent os="win">
          <regex>Windows</regex>
          <sample>Mozilla/5.0 (Windows; U; Windows NT 5.1; en-GB; rv:1.8.0.4) Gecko/20060508 Firefox/1.5.0.4</sample>
        </agent>
      </group>
      <group version="2.0">
        <regex>(Firefox||Iceweasel)/2\.0</regex>
		    <agent os="osx">
		      <regex>Mac OS X</regex>
	        <sample>Mozilla/5.0 (Macintosh; U; Intel Mac OS X; en-US; rv:1.8.1.6) Gecko/20070725 Firefox/2.0.0.6</sample>
		    </agent>
		    <agent os="win">
		      <regex>Windows</regex>
		      <sample>Mozilla/5.0 (Windows; U; Windows NT 5.1; de; rv:1.8.1.20) Gecko/20081217 Firefox/2.0.0.20</sample>
		    </agent>
		    <agent os="linux">
		      <regex>Linux</regex>
		      <sample>Mozilla/5.0 (X11; U; Linux x86_64; en-US; rv:1.8.1.16) Gecko/20080702 Iceweasel/2.0.0.16 (Debian-2.0.0.16-0etch1)</sample>
		    </agent>
		  </group>
      <group version="3.0">
        <regex>(Firefox|Iceweasel|Minefield)/3\.0</regex>
		    <agent os="osx">	
		      <regex>Mac OS X</regex>
	        <sample>Mozilla/5.0 (Macintosh; U; PPC Mac OS X 10.4; en-GB; rv:1.9.0.4) Gecko/2008102920 Firefox/3.0.4</sample>
		    </agent>
		    <agent os="win">
		      <regex>Windows</regex>
		      <sample>Mozilla/5.0 (Windows; U; Windows NT 5.0; en-GB; rv:1.9.0.6) Gecko/2009011913 Firefox/3.0.6</sample>
		      <sample>Mozilla/5.0 (Windows; U; Windows NT 5.1; en-US; rv:1.9.0.9) Gecko/2009040821 Firefox/3.0.9 (.NET CLR 3.5.30729)</sample>
		      <sample>Mozilla/5.0 (Windows; U; Windows NT 5.2; en-US; rv:1.9.0.1) Gecko/2008070208 Firefox/3.0.1</sample>
		      <sample>Mozilla/5.0 (Windows; U; Windows NT 6.0; en-US; rv:1.9.0.11) Gecko/2009060215 Firefox/3.0.11 (.NET CLR 3.5.30729)</sample>
		      <sample>Mozilla/5.0 (Windows; U; Windows NT 6.1; en-GB; rv:1.9.0.10) Gecko/2009042316 Firefox/3.0.10 (.NET CLR 3.5.30729)</sample>
		    </agent>
		    <agent os="linux">
		      <regex>Linux</regex>
		      <sample>Mozilla/5.0 (X11; U; Linux i686; en-GB; rv:1.9.0.7) Gecko/2009030422 Ubuntu/8.10 (intrepid) Firefox/3.0.7</sample>
		      <sample>Mozilla/5.0 (X11; U; Linux i686; en-US; rv:1.9a1) Gecko/20070308 Minefield/3.0a1</sample>
		    </agent>
		  </group>
		  <group version="3.5">
        <regex>(Firefox||Iceweasel)/3\.(5|1b)</regex>
		    <agent os="osx">
		      <regex>Mac OS X</regex>
		  		<sample>Mozilla/5.0 (Macintosh; U; Intel Mac OS X 10.5; en-GB; rv:1.9.1) Gecko/20090624 Firefox/3.5</sample>
		  	</agent>
		    <agent os="win">
		      <regex>Windows</regex>
		      <sample>Mozilla/5.0 (Windows; U; Windows NT 5.1; de; rv:1.9.1) Gecko/20090624 Firefox/3.5</sample>
					<sample>Mozilla/5.0 (Windows; U; Windows NT 5.1; en-US; rv:1.9.1b3) Gecko/20090305 Firefox/3.1b3</sample>
		    </agent>
		    <agent os="linux">
		      <regex>Linux</regex>
		    </agent>
		  </group>
		</group>
		
		<group engine="gecko" name="seamonkey">
      <regex>^Mozilla/5\.0.* Gecko/.* SeaMonkey/.*</regex>
		  <group version="1">
        <regex>SeaMonkey/1\.</regex>
			  <agent os="win">
		      <regex>Windows</regex>
		      <sample>Mozilla/5.0 (Windows; U; Windows NT 5.1; de-AT; rv:1.8.1.21) Gecko/20090403 SeaMonkey/1.1.16</sample>
		    </agent>
		  </group>
		</group>
		
  </group>
  
