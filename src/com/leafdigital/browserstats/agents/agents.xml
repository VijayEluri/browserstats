      <regex>compatible; HTTrack </regex>
      <sample>Mozilla/4.5 (compatible; HTTrack 3.0x; Windows 98)</sample>
    </agent>
    <agent name="sitesucker">
      <regex>^SiteSucker/</regex>
      <sample>SiteSucker/2.2.2</sample>
    </agent>
  </group>
  
  <!-- 
    Software frameworks or robot software that can be used by many people
    for different tasks. 
    -->  
  <group type="software" os="" engine="" version="">
   <agent name="java">
     <regex>^Java/1\.[0-9._]+(-[a-z]+)?$</regex>
     <sample>Java/1.6.0_13</sample>
   </agent>
   <agent name="perl">
     <regex>^libwww-perl/</regex>
